{
	"server": "chat.freenode.net",
	"port": "6667",
	"nick": "fossbot",
	"nick_password": "",
	"user": "bot",
	"quit_message": "segfault",

	// Comma seperated list of channels
	"channels": [ "#foss-teimes" ],

	// String to reply on ctcp version
	"bot_version": "irC Bot - https://github.com/foss-teiwest/irc-bot",

	// Set to false to only show errors
	"verbose": true,

	// Default repo to substitute when not provided for the github command
	"github_repo": "foss-teiwest",

	// Murmur port
	"murmur_port": "6502",

	// MPD settings
	"mpd_port": "6600",
	"mpd_database": "~/Music",
	"mpd_random_state": "~/.mpd_random",

	// Named pipe (FIFO) path
	"fifo_name": "/tmp/irc-bot",

	// Database to put user access, quotes and more
	"db_name": "irc-bot.db",

	// Nicknames allowed to use certain commands like tweet, adding quotes etc
	// Those will be merged with the users already present in the database
	"access_list": [ "freestyl3r" ],

